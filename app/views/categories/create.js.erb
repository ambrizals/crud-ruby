<% if @category.errors.any? %>
    <% @category.errors.each do |f, m| %>
        var error_field = "<div class='error-message alert alert-danger alert-small'><%= m %></div>"
        $('form').find('#category_<%= f %>').after(error_field);
    <% end %>
<% else %>
    console.log('success');
<% end %>